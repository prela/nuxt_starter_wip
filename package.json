{
  "name": "nuxt-enterprise-app",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "test": "pnpm test:unit && pnpm test:e2e && pnpm test:coverage",
    "ci:quality": "pnpm lint && pnpm type-check && pnpm test"
  },
  "dependencies": {
    "@antfu/eslint-config": "^4.16.2",
    "@nuxt/content": "3.6.3",
    "@nuxt/eslint": "^1.5.2",
    "@nuxt/eslint-config": "^1.5.2",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/icon": "1.15.0",
    "@nuxt/image": "1.10.0",
    "@nuxt/scripts": "0.11.8",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxt/ui": "^3.2.0",
    "@pinia/nuxt": "^0.11.1",
    "@vueuse/core": "^13.5.0",
    "@vueuse/nuxt": "^13.5.0",
    "nuxt": "^3.17.6",
    "pinia": "^3.0.3",
    "typescript": "^5.8.3",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.2",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "^9.30.1",
    "happy-dom": "^18.0.1",
    "playwright-core": "^1.53.2",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ]
  }
}
